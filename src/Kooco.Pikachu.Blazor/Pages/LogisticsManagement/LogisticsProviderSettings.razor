                                    </div>
                                    <div class="col-8">
                                        <Select TValue="MainlandCity"
                                                Class="form-select "
                                                @bind-SelectedValue="EcPayHomeDelivery.City">
                                            <SelectItem Value="0"></SelectItem>
                                            @foreach (object enumValue in Enum.GetValues(typeof(MainlandCity)))
                                            {
                                                <SelectItem Value="@enumValue">@L[enumValue.ToString()]</SelectItem>
                                            }
                                        </Select>

                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-3">
                                        <label for="sender-address" class="form-label">@L["SenderAddress"]<span class="text-danger"> </span></label>
                                    </div>
                                    <div class="col-8">
                                        <InputText id="sender-address" class="form-control" @bind-Value="EcPayHomeDelivery.SenderAddress"></InputText>

                                    </div>
                                </div>
                                @if (IsEcPayHomeDeliveryNotExists)
                                {
