                                        <ValidationMessage For="@(() => EcPayHomeDelivery.HashIV)" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-3">
                                        <label for="sender-name" class="form-label">@L["SenderName"]<span class="text-danger"> *</span></label>
                                    </div>
