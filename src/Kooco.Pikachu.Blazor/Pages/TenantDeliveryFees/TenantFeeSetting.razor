@page "/tenant-fee-settings/{TenantId:guid}"
@using Volo.Abp.AspNetCore.Components.Web
@using Volo.Abp.AspNetCore.Components;
@using Microsoft.AspNetCore.Authorization
@using Blazorise
@using Blazorise.DataGrid
@using Kooco.Pikachu.Localization;
@using Kooco.Pikachu.TenantDeliveryFees;
@using Kooco.Pikachu.EnumValues;
@using Volo.Abp.TenantManagement;
@inherits PikachuComponentBase
@using Microsoft.Extensions.Localization;
@inject ITenantDeliveryFeeAppService TenantDeliveryFeeAppService
@inject ITenantAppService TenantAppService;
@inject NavigationManager NavigationManager
@inject IMessageService MessageService
@inject IStringLocalizer<PikachuResource> L;
@* @attribute [Authorize(PikachuPermissions.LogisticsFeeManagement.Default)] *@

<PikachuButton Clicked="NavigateToPlatformManagement">
    <Icon Name="IconName.ArrowLeft" Class="me-1"></Icon>
    @L["PlatformManagement"]
</PikachuButton>
<PikachuPageHeader Breadcrumbs="@(new() { L["PlatformManagement"], L["FeeSettings"],TenantName })"
                   Title=@($"{L["PlatformFeeSettings"]} - {TenantName}") />
                   <Card >
    <Tabs RenderMode="TabsRenderMode.LazyLoad" SelectedTab="Logistic Fees">
            <Items>
                <Tab Name="Logistic Fees">@L["LogisticFees"]</Tab>

            </Items>
            <Content>
        <TabPanel Name="Logistic Fees">
                  <LogisticFeeConfiguration TenantId="TenantId"></LogisticFeeConfiguration> 
                </TabPanel>
              
            </Content>
        </Tabs>
</Card>
